{
  "name": "@tbd54566975/tbdex",
  "version": "0.0.1",
  "description": "Library that includes type definitions for tbdex messages",
  "type": "module",
  "main": "./dist/cjs/main.cjs",
  "module": "./dist/esm/main.mjs",
  "types": "./dist/types/main.d.ts",
  "scripts": {
    "build": "rimraf dist && node build/bundles.js && echo '{\"type\": \"commonjs\"}' > ./dist/cjs/package.json && tsc",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "homepage": "https://github.com/TBD54566975/tbdex-protocol/tree/main/protocol#readme",
  "bugs": "https://github.com/TBD54566975/tbdex-protocol/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TBD54566975/tbdex-protocol",
    "directory": "implementations/js"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Jiyoon Koo",
      "url": "https://github.com/jiyoontbd"
    },
    {
      "name": "Moe Jangda",
      "url": "https://github.com/mistermoe"
    }
  ],
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/main.mjs",
      "require": "./dist/cjs/main.cjs",
      "types": "./dist/types/main.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.js",
      "types": "./dist/types/main.d.ts"
    }
  },
  "browser": {
    "./dist/esm/main.mjs": "./dist/browser.mjs",
    "./dist/cjs/main.cjs": "./dist/browser.js",
    "types": "./dist/types/main.d.ts"
  },
  "keywords": [],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/eslint": "8.37.0",
    "esbuild": "0.16.17",
    "node-stdlib-browser": "1.2.0",
    "rimraf": "4.4.0",
    "sinon": "15.0.2",
    "typescript": "5.0.4"
  }
}